{
  "name": "example",
  "version": "1.0.0",
  "description": "NoteBot示例插件配置文件",
  "main": "index.js",
  "enabled": true,
  "priority": 100,
  "dependencies": [
    "base",
    "puppeteer"
  ],
  "permissions": [
    "message.send",
    "message.receive",
    "config.read",
    "config.write",
    "api.call",
    "puppeteer.use",
    "scheduler.manage"
  ],
  "settings": {
    "param1": "示例字符串参数",
    "param2": 42,
    "required_param": "这是必需的配置参数",
    "greeting": "你好",
    "welcome_enabled": true,
    "welcome_message": "欢迎加入我们的群组！",
    "api_base_url": "https://jsonplaceholder.typicode.com",
    "timeout": 30000,
    "max_retries": 3,
    "debug_mode": false,
    "log_level": "info",
    "features": {
      "auto_reply": true,
      "command_logging": true,
      "user_tracking": false,
      "api_integration": true
    },
    "limits": {
      "max_message_length": 1000,
      "rate_limit": 10,
      "concurrent_requests": 5
    },
    "ui_config": {
      "theme": "default",
      "language": "zh-CN",
      "show_advanced": false,
      "auto_save": true
    }
  },
  "notebot": {
    "commands": [
      {
        "name": "hello",
        "description": "发送问候消息",
        "usage": "#hello [名字]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "config",
        "description": "查看或设置插件配置",
        "usage": "#config [参数] [值]",
        "enabled": true,
        "permission_required": true
      },
      {
        "name": "status",
        "description": "查看插件状态",
        "usage": "#status",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "api",
        "description": "调用示例API",
        "usage": "#api [endpoint]",
        "enabled": true,
        "permission_required": true
      },
      {
        "name": "resource",
        "description": "读取示例资源",
        "usage": "#resource [文件名]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "screenshot",
        "description": "网页截图命令",
        "usage": "#screenshot <url> [width] [height]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "schedule",
        "description": "定时任务管理命令",
        "usage": "#schedule <list|add|remove|status> [name] [cron] [message]",
        "enabled": true,
        "permission_required": true
      }
    ]
  },
  "events": [
    {
      "name": "message",
      "handler": "onMessageReceived",
      "enabled": true
    },
    {
      "name": "user.join",
      "handler": "onUserJoin",
      "enabled": true
    },
    {
      "name": "user.leave",
      "handler": "onUserLeave",
      "enabled": true
    },
    {
      "name": "config.update",
      "handler": "onConfigUpdate",
      "enabled": true
    }
  ],
  "web_interface": {
    "enabled": true,
    "path": "/plugins/example",
    "title": "示例插件管理",
    "description": "示例插件的配置和管理界面",
    "icon": "fas fa-cog",
    "menu_order": 100
  },
  "notebot": {
    "config": {
      "default": {
        "settings": {
          "param1": "示例字符串参数",
          "param2": 42,
          "required_param": "这是必需的配置参数",
          "greeting": "你好",
          "welcome_enabled": true,
          "welcome_message": "欢迎加入我们的群组！",
          "api_base_url": "https://jsonplaceholder.typicode.com",
          "timeout": 30000,
          "max_retries": 3,
          "debug_mode": false,
          "log_level": "info",
          "features": {
            "auto_reply": true,
            "command_logging": true,
            "user_tracking": false,
            "api_integration": true
          },
          "limits": {
            "max_message_length": 1000,
            "rate_limit": 10,
            "concurrent_requests": 5
          },
          "ui_config": {
            "theme": "default",
            "language": "zh-CN",
            "show_advanced": false,
            "auto_save": true
          }
        }
      }
    },
    "commands": [
      {
        "name": "hello",
        "description": "发送问候消息",
        "usage": "#hello [用户名]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "config",
        "description": "配置管理命令",
        "usage": "#config [参数] [值]",
        "enabled": true,
        "permission_required": true
      },
      {
        "name": "status",
        "description": "查看插件状态",
        "usage": "#status",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "resource",
        "description": "获取插件资源",
        "usage": "#resource [文件名]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "screenshot",
        "description": "网页截图命令",
        "usage": "#screenshot <url> [width] [height]",
        "enabled": true,
        "permission_required": false
      },
      {
        "name": "schedule",
        "description": "定时任务管理命令",
        "usage": "#schedule <list|add|remove|status> [name] [cron] [message]",
        "enabled": true,
        "permission_required": true
      }
    ]
  },
